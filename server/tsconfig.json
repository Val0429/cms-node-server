{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "lib/*": [
        "./../lib/*"
      ]
    },
    "outDir": "./dist",
    "noImplicitAny": false,
    "sourceMap": false,
    "declaration": true,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "commonjs",
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "types": [
      "parse",
      "nconf"
    ],
    "lib": [
      "es2017",
      "dom"
    ]
  }
}